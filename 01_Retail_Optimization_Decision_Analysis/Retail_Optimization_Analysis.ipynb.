    {
      "cell_type": "code",
      "execution_count": 109,
      "metadata": {},
      "outputs": [],
      "source": [
        "def productos_mas_vendidos(tiendas, nombres_tiendas):\n",
        "    top5_productos = pd.concat(tiendas)['Producto'].value_counts().head(5).index\n",
        "    \n",
        "    datos = []\n",
        "    \n",
        "    for producto in top5_productos:\n",
        "        fila = {'Producto': producto}\n",
        "        \n",
        "        for nombre, tienda in zip(nombres_tiendas, tiendas):\n",
        "            fila[nombre] = len(tienda[tienda['Producto'] == producto])\n",
        "        \n",
        "        fila['Total Global'] = sum(fila[nombre] for nombre in nombres_tiendas)\n",
        "        datos.append(fila)\n",
        "    \n",
        "    tabla_final = pd.DataFrame(datos)\n",
        "    tabla_final = tabla_final.sort_values('Total Global', ascending=False)\n",
        "    \n",
        "    return tabla_final"
      ]
    },
