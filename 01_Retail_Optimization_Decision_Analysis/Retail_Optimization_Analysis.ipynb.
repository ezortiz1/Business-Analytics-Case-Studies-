      "source": [
        "def venta_anual(tienda):\n",
        "  venta = tienda.groupby(tienda[\"Fecha de Compra\"].dt.year)[\"Precio\"].sum()\n",
        "  venta = venta.to_frame(\"Venta anual\")\n",
        "  return venta"
      ]
    },
