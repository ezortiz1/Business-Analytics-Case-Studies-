        "id": "kf3-xsOQogFk"
      },
      "outputs": [],
      "source": [
        "def promedio_calificacion(tienda):\n",
        "  diccionario = {calificacion: cantidad for calificacion, cantidad in tienda[\"Calificación\"].value_counts().items()}\n",
        "\n",
        "  rpta = 0\n",
        "  for clave, valor in diccionario.items():\n",
        "    rpta += clave * valor\n",
        "\n",
        "  rpta = rpta / sum(diccionario.values())\n",
        "  return round(rpta,2)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 91,
      "metadata": {
        "colab": {
          "background_save": true
        },
        "id": "aXmtKgFi-gLm"
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "3.98\n"
          ]
        }
      ],
      "source": [
        "print(promedio_calificacion(tienda1))"
      ]
    },
