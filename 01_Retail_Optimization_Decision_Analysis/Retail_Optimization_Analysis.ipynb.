        },
        "id": "80g5I0uafv79"
      },
      "outputs": [],
      "source": [
        "def tendencia_anual(tienda, nombre_tienda):\n",
        "    ventas = venta_anual(tienda).sort_index()\n",
        "\n",
        "    fig, ax = plt.subplots()\n",
        "\n",
        "    ax.plot(ventas.index, ventas[\"Venta anual\"],\n",
        "            marker='o', linestyle='--', color='green')\n",
        "\n",
        "    # Configurar eje X para mostrar solo años enteros\n",
        "    ax.set_xticks(ventas.index)  # Fijar ticks exactos en los años existentes\n",
        "    ax.set_xticklabels(ventas.index.astype(int))  # Mostrar como enteros\n",
        "\n",
        "    plt.title(f'Tendencia Anual - {nombre_tienda}')\n",
        "    plt.xlabel('Año')\n",
        "    plt.ylabel('Ventas')\n",
        "    plt.grid()\n",
        "    plt.show()"
      ]
    },
