      "outputs": [],
      "source": [
        "def tendencia_ventas(tienda, nombre_tienda):\n",
        "  ventas = cantidad_ventas_anuales(tienda).sort_index()\n",
        "\n",
        "  fig, ax = plt.subplots()\n",
        "\n",
        "  ax.plot(ventas.index, ventas[\"Cantidad de ventas\"],\n",
        "          marker=\"o\", linestyle=\"--\", color=\"blue\")\n",
        "\n",
        "  ax.set_xticks(ventas.index)\n",
        "  ax.set_xticklabels(ventas.index.astype(int))\n",
        "\n",
        "  plt.title(f\"Tendencia Cantidad de ventas anual - {nombre_tienda}\")\n",
        "  plt.xlabel(\"Año\")\n",
        "  plt.ylabel(\"Cantidad de venntas anuales\")\n",
        "  plt.grid()\n",
        "  plt.show()"
      ]
    },
