    {
      "cell_type": "code",
      "execution_count": 112,
      "metadata": {},
      "outputs": [],
      "source": [
        "def productos_menos_vendidos(tiendas, nombres_tiendas):\n",
        "    datos = []\n",
        "\n",
        "    productos = pd.concat(tiendas)[\"Producto\"].value_counts().tail(5).index\n",
        "\n",
        "    for producto in productos:\n",
        "        fila = {\"Producto\": producto}\n",
        "        for tienda, nombre in zip(tiendas, nombres_tiendas):\n",
        "            fila[nombre] = len(tienda[tienda[\"Producto\"]==producto])\n",
        "        \n",
        "        fila[\"Total Global\"] = sum(fila[nombre] for nombre in nombres_tiendas)\n",
        "        datos.append(fila)\n",
        "    \n",
        "    tabla_final = pd.DataFrame(datos)\n",
        "    tabla_final = tabla_final.sort_values('Total Global', ascending=False)\n",
        "\n",
        "    return tabla_final"
      ]
    },
