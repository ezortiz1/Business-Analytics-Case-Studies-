      "cell_type": "code",
      "execution_count": 87,
      "metadata": {},
      "outputs": [],
      "source": [
        "def ventas_por_categoria_por_tienda(tiendas, nombres_tiendas):\n",
        "    lista_dataframes = []\n",
        "    categorias = tiendas[0][\"Categoría del Producto\"].unique()\n",
        "    \n",
        "    for i in range(len(tiendas)):\n",
        "        tienda_df = tiendas[i]\n",
        "        nombre_tienda = nombres_tiendas[i]\n",
        "        \n",
        "        porcentajes = {}\n",
        "        \n",
        "        total_ventas = tienda_df['Precio'].sum()\n",
        "        \n",
        "        for categoria in categorias:\n",
        "            ventas_categoria = tienda_df[tienda_df['Categoría del Producto'] == categoria]['Precio'].sum()\n",
        "            \n",
        "            porcentaje = round((ventas_categoria / total_ventas) * 100, 2)\n",
        "                \n",
        "            porcentajes[categoria] = f\"{porcentaje}%\"\n",
        "        \n",
        "        df_tienda = pd.DataFrame([porcentajes], index=[nombre_tienda])\n",
        "        lista_dataframes.append(df_tienda)\n",
        "    \n",
        "    tabla_final = pd.concat(lista_dataframes)\n",
        "    \n",
        "    return tabla_final"
      ]
    },
    {
