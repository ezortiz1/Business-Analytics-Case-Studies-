      "cell_type": "code",
      "execution_count": 95,
      "metadata": {},
      "outputs": [],
      "source": [
        "def grafico_promedio_calificacion(lista_tienda, nombres_tiendas):\n",
        "    promedios = [promedio_calificacion(tienda) for tienda in lista_tienda]\n",
        "    \n",
        "    plt.figure(figsize=(10, 5))\n",
        "    barras = plt.bar(nombres_tiendas, promedios, color=['#FF6B6B', '#4ECDC4', '#45B7D1', '#FFA07A'])\n",
        "    \n",
        "    plt.title('Promedio de Calificaciones por Tienda\\n', fontsize=14, fontweight='bold')\n",
        "    plt.xlabel('Tiendas', fontsize=12)\n",
        "    plt.ylim(0, 5.5)\n",
        "    plt.grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "    # Añadir valores en las barras\n",
        "    for bar in barras:\n",
        "        height = bar.get_height()\n",
        "        plt.text(bar.get_x() + bar.get_width()/2., height-0.3,\n",
        "                f'{height:.2f}',\n",
        "                ha='center', va='bottom',\n",
        "                color='white', fontweight='bold')\n",
        "    \n",
        "    plt.show()"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 96,
      "metadata": {},
      "outputs": [
        {
          "data": {
            "image/png": "",
